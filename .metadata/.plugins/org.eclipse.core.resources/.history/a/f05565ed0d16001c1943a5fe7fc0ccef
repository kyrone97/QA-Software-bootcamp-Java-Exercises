
public class ArraysExercise {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// initialise empty array of longs that can store up to 6 values
		
		long[] ageArray =  new long[6];
		
		// assign value to ageArray at index 2
		
		ageArray[2] = 16;
		// assign value to ageArray at index 5
		ageArray[5] = 17;
		
		ageArray[0] = 10;
		ageArray[1] = 12;
		ageArray[3] = 6;
		ageArray[4] = 10;
		
		// use for-each loop to print out all values to console
		
		for(long i : ageArray) {
			System.out.println(i);
		}




		


		
		
		

	}

}